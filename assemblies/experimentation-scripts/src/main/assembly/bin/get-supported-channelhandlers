#!/bin/bash
BASEDIR=`dirname $0`

if [ "$#" -lt 2 ]
then
	echo "Usage $0 PROPERTIES_FILE SECRET_RESERVATION_KEYS"
	echo "Example: $0 live.properties urn:wisebed:uzl1:,D2C88AEE416EDD2FD4038BC1252A8900"
	exit 1
fi

java \
 -Dtestbed.secretreservationkeys=$2 \
 -cp ../lib/tr.scripting-client-0.7.2-SNAPSHOT-onejar.jar \
 de.uniluebeck.itm.wisebed.cmdlineclient.scripts.GetSupportedChannelHandlers \
 $1
