#!/bin/bash
BASEDIR=`dirname $0`

if [ "$#" -lt 3 ]
then
	echo "Usage $0 PROPERTIES_FILE SECRET_RESERVATION_KEYS CONFIG_FILE [NODEURNS]"
	echo "Example: $0 live.properties urn:wisebed:uzl1:,D2C88AEE416EDD2FD4038BC1252A8900 handlerchain.xml urn:wisebed:uzl1:0xcc3d,urn:wisebed:uzl1:0xcc3e"
	exit 1
fi

java \
 -Dtestbed.secretreservationkeys=$2 \
 -Dtestbed.configfile=$3 \
 -Dtestbed.nodeurns=$4 \
 -Dtestbed.useprotobuf=true \
 -cp ../lib/tr.scripting-client-0.7.2-SNAPSHOT-onejar.jar \
 de.uniluebeck.itm.wisebed.cmdlineclient.scripts.SetChannelPipeline \
 $1
