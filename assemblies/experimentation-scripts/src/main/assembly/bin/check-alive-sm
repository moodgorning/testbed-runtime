#!/bin/bash
BASEDIR=`dirname $0`

if [ "$#" -lt 1 ]
then
	echo "Usage $0 PROPERTIES_FILE [NODEURNS]"
	echo "Example: $0 live.properties urn:wisebed:uzl1:0xf859,urn:wisebed:uzl1:0xf860"
	exit 1
fi

java -Dtestbed.nodeurns=$2 -jar $BASEDIR/../lib/tr.scripting-client-${project.version}-onejar.jar -p $1 -f $BASEDIR/../scripts/check-alive-sm.java
